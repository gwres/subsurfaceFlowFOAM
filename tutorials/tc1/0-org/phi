FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location	"0";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
	bottom
    {
        type            calculated;
        value           uniform 0.0;
    }
    top
    {
        type            calculated;
        value           uniform 0.0;
    }
    defaultFaces
    {
        type            empty;
    }
}

