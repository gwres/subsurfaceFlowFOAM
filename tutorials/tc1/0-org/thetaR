FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location 	"0";
    object      thetaR;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.078;

boundaryField
{
	bottom
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}

